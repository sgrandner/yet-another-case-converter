{
    "name": "yet-another-case-converter",
    "displayName": "Yet Another Case Converter",
    "description": "Change/toggle the case of the selected word(s)",
    "publisher": "sgrandner",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.75.0"
    },
    "categories": [
        "Formatters"
    ],
    "main": "./dist/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "lint:fix": "npm run lint -- --fix",
        "test": "node ./out/test/runTest.js"
    },
    "contributes": {
        "commands": [
            {
                "command": "yet-another-case-converter.upper-snake-case",
                "title": "Yet Another Case Converter: CONSTANT_CASE / SCREAMING_SNAKE_CASE (Constant Case, Screaming Snake Case)",
                "enablement": "config.yet-another-case-converter.activate.upper-snake-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.lower-snake-case",
                "title": "Yet Another Case Converter: snake_case (Snake Case, Pothole Case)",
                "enablement": "config.yet-another-case-converter.activate.lower-snake-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.each-first-upper-snake-case",
                "title": "Yet Another Case Converter: Pascal_Snake_Case (Pascal Snake Case)",
                "enablement": "config.yet-another-case-converter.activate.each-first-upper-snake-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.each-first-lower-snake-case",
                "title": "Yet Another Case Converter: iNVERSE_pASCAL_sNAKE_cASE (inverse Pascal Snake Case)",
                "enablement": "config.yet-another-case-converter.activate.each-first-lower-snake-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.upper-double-snake-case",
                "title": "Yet Another Case Converter: LONG__SCREAMING__SNAKE__CASE (long Screaming Snake Case)",
                "enablement": "config.yet-another-case-converter.activate.upper-double-snake-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.lower-double-snake-case",
                "title": "Yet Another Case Converter: long__snake__case (long Snake Case)",
                "enablement": "config.yet-another-case-converter.activate.lower-double-snake-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.each-first-upper-double-snake-case",
                "title": "Yet Another Case Converter: Long__Pascal__Snake__Case (long Pascal Snake Case)",
                "enablement": "config.yet-another-case-converter.activate.each-first-upper-double-snake-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.each-first-lower-double-snake-case",
                "title": "Yet Another Case Converter: iNVERSE__lONG__pASCAL__sNAKE__cASE (inverse long Pascal Snake Case)",
                "enablement": "config.yet-another-case-converter.activate.each-first-lower-double-snake-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.upper-kebab-case",
                "title": "Yet Another Case Converter: COBOL-CASE (Cobol Case, Screaming Kebab Case)",
                "enablement": "config.yet-another-case-converter.activate.upper-kebab-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.lower-kebab-case",
                "title": "Yet Another Case Converter: kebab-case (Kebab Case, Dash Case)",
                "enablement": "config.yet-another-case-converter.activate.lower-kebab-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.each-first-upper-kebab-case",
                "title": "Yet Another Case Converter: Train-Case (Train Case, HTTP Header Case)",
                "enablement": "config.yet-another-case-converter.activate.each-first-upper-kebab-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.each-first-lower-kebab-case",
                "title": "Yet Another Case Converter: tRAIN-cASE (inverse Train Case)",
                "enablement": "config.yet-another-case-converter.activate.each-first-lower-kebab-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.upper-double-kebab-case",
                "title": "Yet Another Case Converter: LONG--SCREAMING--KEBAB--CASE (long Screaming Kebab Case)",
                "enablement": "config.yet-another-case-converter.activate.upper-double-kebab-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.lower-double-kebab-case",
                "title": "Yet Another Case Converter: long--kebab--case (long Kebab Case, double Dash Case)",
                "enablement": "config.yet-another-case-converter.activate.lower-double-kebab-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.each-first-upper-double-kebab-case",
                "title": "Yet Another Case Converter: Long--Train--Case (long Train Case)",
                "enablement": "config.yet-another-case-converter.activate.each-first-upper-double-kebab-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.each-first-lower-double-kebab-case",
                "title": "Yet Another Case Converter: iNVERSE--lONG--tRAIN--cASE (inverse long Train Case)",
                "enablement": "config.yet-another-case-converter.activate.each-first-lower-double-kebab-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.upper-space-case",
                "title": "Yet Another Case Converter: SCREAMING SPACE CASE (Screaming Space Case)",
                "enablement": "config.yet-another-case-converter.activate.upper-space-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.lower-space-case",
                "title": "Yet Another Case Converter: lower space case (Lower Space Case)",
                "enablement": "config.yet-another-case-converter.activate.lower-space-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.each-first-upper-space-case",
                "title": "Yet Another Case Converter: Title Case",
                "enablement": "config.yet-another-case-converter.activate.each-first-upper-space-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.each-first-lower-space-case",
                "title": "Yet Another Case Converter: iNVERSE tITLE cASE (Inverse Title Case)",
                "enablement": "config.yet-another-case-converter.activate.each-first-lower-space-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.upper-dot-case",
                "title": "Yet Another Case Converter: SCREAMING.DOT.CASE (Screaming Dot Case)",
                "enablement": "config.yet-another-case-converter.activate.upper-dot-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.lower-dot-case",
                "title": "Yet Another Case Converter: lower.dot.case (Lower Dot Case)",
                "enablement": "config.yet-another-case-converter.activate.lower-dot-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.each-first-upper-dot-case",
                "title": "Yet Another Case Converter: Pascal.Dot.Case (Pascal Dot Case)",
                "enablement": "config.yet-another-case-converter.activate.each-first-upper-dot-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.each-first-lower-dot-case",
                "title": "Yet Another Case Converter: iNVERSE.pASCAL.dOT.cASE (inverse Pascal Dot Case)",
                "enablement": "config.yet-another-case-converter.activate.each-first-lower-dot-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.upper-camel-case",
                "title": "Yet Another Case Converter: PascalCase (Pascal Case, Upper Camel Case, Studly Case)",
                "enablement": "config.yet-another-case-converter.activate.upper-camel-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.lower-camel-case",
                "title": "Yet Another Case Converter: camelCase (Lower Camel Case, Dromedary Case)",
                "enablement": "config.yet-another-case-converter.activate.lower-camel-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.lower-inverse-camel-case",
                "title": "Yet Another Case Converter: iNVERSEpASCALcASE (inverse Pascal Case)",
                "enablement": "config.yet-another-case-converter.activate.lower-inverse-camel-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.upper-case",
                "title": "Yet Another Case Converter: UPPER CASE (Upper Case)",
                "enablement": "config.yet-another-case-converter.activate.upper-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.lower-case",
                "title": "Yet Another Case Converter: lower case (Lower Case)",
                "enablement": "config.yet-another-case-converter.activate.lower-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.upper-custom1-case",
                "title": "Yet Another Case Converter: SCREAMING CUSTOM1 CASE (as configured)",
                "enablement": "config.yet-another-case-converter.custom1-separator && config.yet-another-case-converter.activate.upper-custom1-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.lower-custom1-case",
                "title": "Yet Another Case Converter: lower custom1 case (as configured)",
                "enablement": "config.yet-another-case-converter.custom1-separator && config.yet-another-case-converter.activate.lower-custom1-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.each-first-upper-custom1-case",
                "title": "Yet Another Case Converter: Pascal Custom1 Case (as configured)",
                "enablement": "config.yet-another-case-converter.custom1-separator && config.yet-another-case-converter.activate.each-first-upper-custom1-case == true && editorHasSelection"
            },
            {
                "command": "yet-another-case-converter.each-first-lower-custom1-case",
                "title": "Yet Another Case Converter: iNVERSE pASCAL cUSTOM1 cASE (as configured)",
                "enablement": "config.yet-another-case-converter.custom1-separator && config.yet-another-case-converter.activate.each-first-lower-custom1-case == true && editorHasSelection"
            }
        ],
        "configuration": {
            "title": "Yet Another Case Converter",
            "properties": {
                "yet-another-case-converter.activate.upper-snake-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate upper-snake-case command"
                },
                "yet-another-case-converter.activate.lower-snake-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate lower-snake-case command"
                },
                "yet-another-case-converter.activate.each-first-upper-snake-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate each-first-upper-snake-case command"
                },
                "yet-another-case-converter.activate.each-first-lower-snake-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate each-first-lower-snake-case command"
                },
                "yet-another-case-converter.activate.upper-double-snake-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate upper-double-snake-case command"
                },
                "yet-another-case-converter.activate.lower-double-snake-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate lower-double-snake-case command"
                },
                "yet-another-case-converter.activate.each-first-upper-double-snake-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate each-first-upper-double-snake-case command"
                },
                "yet-another-case-converter.activate.each-first-lower-double-snake-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate each-first-lower-double-snake-case command"
                },
                "yet-another-case-converter.activate.upper-kebab-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate upper-kebab-case command"
                },
                "yet-another-case-converter.activate.lower-kebab-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate lower-kebab-case command"
                },
                "yet-another-case-converter.activate.each-first-upper-kebab-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate each-first-upper-kebab-case command"
                },
                "yet-another-case-converter.activate.each-first-lower-kebab-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate each-first-lower-kebab-case command"
                },
                "yet-another-case-converter.activate.upper-space-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate upper-space-case command"
                },
                "yet-another-case-converter.activate.lower-space-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate lower-space-case command"
                },
                "yet-another-case-converter.activate.each-first-upper-space-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate each-first-upper-space-case command"
                },
                "yet-another-case-converter.activate.each-first-lower-space-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate each-first-lower-space-case command"
                },
                "yet-another-case-converter.activate.upper-dot-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate upper-dot-case command"
                },
                "yet-another-case-converter.activate.lower-dot-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate lower-dot-case command"
                },
                "yet-another-case-converter.activate.each-first-upper-dot-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate each-first-upper-dot-case command"
                },
                "yet-another-case-converter.activate.each-first-lower-dot-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate each-first-lower-dot-case command"
                },
                "yet-another-case-converter.activate.upper-camel-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate upper-camel-case command"
                },
                "yet-another-case-converter.activate.lower-camel-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate lower-camel-case command"
                },
                "yet-another-case-converter.activate.lower-inverse-camel-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate lower-inverse-camel-case command"
                },
                "yet-another-case-converter.activate.upper-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate upper-case command"
                },
                "yet-another-case-converter.activate.lower-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate lower-case command"
                },
                "yet-another-case-converter.custom1-separator": {
                    "type": "string",
                    "default": "",
                    "description": "Define custom1-separator"
                },
                "yet-another-case-converter.activate.upper-custom1-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate upper-custom1-case command"
                },
                "yet-another-case-converter.activate.lower-custom1-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate lower-custom1-case command"
                },
                "yet-another-case-converter.activate.each-first-upper-custom1-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate each-first-upper-custom1-case command"
                },
                "yet-another-case-converter.activate.each-first-lower-custom1-case": {
                    "type": "boolean",
                    "default": true,
                    "description": "Activate each-first-lower-custom1-case command"
                }
            }
        }
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@types/sinon": "^10.0.13",
        "@types/vscode": "^1.63.0",
        "@typescript-eslint/parser": "^5.9.1",
        "@vscode/test-electron": "^2.0.3",
        "eslint": "^8.6.0",
        "eslint-plugin-import-newlines": "^1.3.4",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-unused-imports": "^3.0.0",
        "glob": "^7.2.0",
        "mocha": "^9.1.3",
        "sinon": "^15.0.1",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.4",
        "webpack": "^5.66.0",
        "webpack-cli": "^4.9.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sgrandner/yet-another-case-converter.git"
    }
}
